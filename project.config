[project]
	description = Data capture webservices
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Dave Martin (djtfmartin)
[access "refs/for/refs/*"]
	read = group user/Dave Martin (djtfmartin)
	push = group user/Dave Martin (djtfmartin)
	label-Code-Review = -2..+2 group user/Dave Martin (djtfmartin)
	label-Verified = -1..+1 group user/Dave Martin (djtfmartin)
	rebase = group user/Dave Martin (djtfmartin)
	removeReviewer = group user/Dave Martin (djtfmartin)
	submit = group user/Dave Martin (djtfmartin)
[access "refs/heads/*"]
	read = group user/Dave Martin (djtfmartin)
	create = group user/Dave Martin (djtfmartin)
	push = +force group user/Dave Martin (djtfmartin)
	pushMerge = group user/Dave Martin (djtfmartin)
[access "refs/tags/*"]
	push = +force group user/Dave Martin (djtfmartin)
	pushTag = group user/Dave Martin (djtfmartin)
	pushSignedTag = group user/Dave Martin (djtfmartin)
